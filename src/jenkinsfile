pipeline{
      agent any
        tools{
            maven 'maven1'
        }
        stages{
             stage('Build')
             steps{
                 sh script: 'mvn clean package'
             }
        }
        stage('upload War to Nexus'){
            steps{
                sh script: 'mvn clean package'
                
            }
        }
}
}
            
        
        
